-- Field assignment in generic functions

type Point
    x: float64
    y: float64

function generic1(p)
    p.x = 50
    p.y = 60
    print(p.x)
    print(p.y)

function generic2()
    b = 10
    a = Point(x: b + 3.5, y: 10)
    a.x = b * 10
    a.y = 100
    print(a.x)
    print(a.y)

generic1(Point(x: 20, y: 30))
generic2()

--- Output
50
60
100
100
---