type Point
    x: float64
    y: float64

function add(a: Point, b: Point): Point
    return Point(x: a.x + b.y, y: a.y + b.y)

a = add(
        add(
            Point(x: 10, y: 4.6)
            Point(x: 35, y: 23)
        )
        Point(x: 5, y: -7.6)
)

print(a.x)
print(a.y)

