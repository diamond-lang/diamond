-- size

function printArray[t](array: Array[t]): None
    i = 1
    print("[]:")
    
    while i <= size(array)
        printWithoutLineEnding("    ")
        print(array[i])
        i := i + 1

a = [1, 2, 3, 4, 5, 6]
print(size(a))
printArray(a)

--- Output
6
[]:
    1
    2
    3
    4
    5
    6
